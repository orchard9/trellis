# Trellis Campaigns Frontend Configuration
# Campaign management and analytics UI

# Development server
VITE_PORT=3000
VITE_HOST=localhost

# API endpoints
VITE_API_BASE_URL=http://localhost:8080
VITE_WAREHOUSE_API_URL=http://localhost:8081
VITE_INGRESS_API_URL=http://localhost:8080

# Warden authentication
VITE_WARDEN_API_URL=http://localhost:21382
VITE_AUTH_REDIRECT_URI=http://localhost:3000/auth/callback

# Feature flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_EXPORTS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_PATTERN_DISCOVERY=true

# UI configuration
VITE_APP_NAME="Trellis Campaigns"
VITE_APP_VERSION="1.0.0"
VITE_THEME=light
VITE_DEFAULT_TIMEZONE=UTC

# Development settings
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# Analytics and tracking
VITE_ENABLE_TELEMETRY=false
VITE_SENTRY_DSN=

# Performance
VITE_QUERY_CACHE_TIME=300000
VITE_STALE_TIME=30000
VITE_RETRY_COUNT=3